<%- css_ex(theme.static_prefix.bootstrap, 'css/bootstrap.min.css') %>

<% if (page.css_snippets) { %>
  <% for (var idx = 0; idx < page.css_snippets.length; idx++) { %>
    <%- page.css_snippets[idx] %>
  <% } %>
  <% page.css_snippets = [] %>
<% } %>

